<div class="row sign-up-form">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%= @student.name %></span>
        <p><%= @student.email %></p>
      </div>
      <div class="card-action">
        <%= link_to 'View Profile', '#' %>
        <%= link_to '<i class="material-icons right">create</i>Edit your profile'.html_safe, edit_student_path(@student), class: "waves-effect waves-light btn" %>
      </div>
    </div>
  </div>
</div>
